<Window x:Class="DayZ_Server_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DayZ_Server_Manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Name="ServerModsStage" Grid.ColumnSpan="1">
            <TextBlock>Server Mods</TextBlock>
        </StackPanel>
        <StackPanel Name="ClientModsStage" Grid.Column="1">
            <TextBlock>Client Mods</TextBlock>
        </StackPanel>
    </Grid>
</Window>
